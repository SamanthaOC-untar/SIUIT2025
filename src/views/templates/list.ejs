<table>
  <tbody>
    <% if (pengeluaran.length > 0) { %>
      <% pengeluaran.forEach((item, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= item.sumber %></td>
          <td><%= format.rupiah(item.jumlah) %></td>  <!-- Gunakan helper -->
          <td><%= item.kategori %></td>
          <td><%= format.tanggal(item.tanggal) %></td>
        </tr>
      <% }); %>
    <% } else { %>
      <tr>
        <td colspan="5">Belum ada data</td>
      </tr>
    <% } %>
  </tbody>
</table>